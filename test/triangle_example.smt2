(set-logic QF_LIA)
(set-info :ran/ran2.smt2)
(set-info :smt-lib-version 2.0)
(declare-fun a () Int)
(declare-fun b () Int)
(declare-fun c () Int)
(assert (and (>= a 1) (<= a 32)) )
(assert (and (>= b 1) (<= b 32)) )
(assert (and (>= c 1) (<= c 32)) )
(assert (or (= a b) (= a c) (= b c)) )
(assert (> (+ a b) c) )
(assert (> (+ a c) b) )
(assert (> (+ b c) a) )
(check-sat)
(exit)
